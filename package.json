{
  "name": "wedding-admin-proxy",
  "version": "1.0.0",
  "description": "Proxy server for Framer wedding admin",
  "main": "index.js",
  "scripts": {
    "dev": "echo 'Use npm run dev:qa for QA environment or direct vercel commands'",
    "deploy": "vercel --prod",
    "deploy:qa": "VERCEL_CLI_SKIP_DEV_SCRIPT=true npx vercel --prod --local-config=vercel.qa.json",
    "dev:qa": "VERCEL_CLI_SKIP_DEV_SCRIPT=true npx vercel dev --local-config=vercel.qa.json",
    "build": "vercel build",
    "preview": "vercel dev --listen 3001"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.83.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer": "^2.4.1",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.34",
    "typescript": "^5.0.2",
    "vercel": "^47.0.1"
  }
}
